{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'unsafe-eval' https://accounts.spotify.com https://accounts.scdn.co; connect-src https://gew1-spclient.spotify.com"
        }
      ]
    }
  ]
}
